   pmid = {17660459},
   publisher = {BMJ Publishing Group Ltd},
   title = {Germline E-cadherin mutations in familial lobular breast cancer},
   volume = {44},
   url = {https://jmg.bmj.com/content/44/11/726 https://jmg.bmj.com/content/44/11/726.abstract},
   year = {2007},
}

