   journal = {Medicine (United States)},
   keywords = {conference,diagnosis,diagnostic techniques and procedures,oncology,therapeutics},
   month = {12},
   pmid = {29310324},
   publisher = {Lippincott Williams and Wilkins},
   title = {Efficacy of the multidisciplinary tumor board conference in gynecologic oncology},
   volume = {96},
   url = {https://journals.lww.com/md-journal/Fulltext/2017/12010/Efficacy_of_the_multidisciplinary_tumor_board.4.aspx},
