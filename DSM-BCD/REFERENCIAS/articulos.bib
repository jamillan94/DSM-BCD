number = {7788},
pages = {89--94},
pmid = {31894144},
publisher = {Springer US},
title = {{International evaluation of an AI system for breast cancer screening}},
url = {http://dx.doi.org/10.1038/s41586-019-1799-6},
volume = {577},
year = {2020}
}

